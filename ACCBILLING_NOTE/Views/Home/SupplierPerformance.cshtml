<script>
    document.addEventListener("keyup", function (event) {
        if (event.keyCode === 13) {
            queryQuotation();
        }
    });

</script>


<script>

    function queryQuotation() {

        var YearVal = $("#Yer").val();
        var supllierNo = $("#supllierNo").val();

        var permission = [];
        $.each($("input[name='lists[chkCreate]']:checked"), function () {
            permission.push($(this).val());


        });

        var fileData = new FormData();

        fileData.append('YearVal', YearVal);
        fileData.append('permission', permission);
        fileData.append('supllierNo', supllierNo);


        $.ajax({
            url: '/Home/SupplierPerformance',
            type: "POST",
            contentType: false,
            processData: false,
            data: fileData,
            success: function (result) {
                $("#Tacl tr").remove();
                var item = '', rows, rowss;
                $.each(result, function (i, item) {

                    rows = "<tr style='font-size:11px;'>" +
                           "<td  align='center'>" + (i + 1) + "</td>" +
                           "<td><a href='~/File/Performance/" + item.remark1 + "target='_blank'><img src='data:image/png;base64,' alt='PDF' style='width:20px' /></a></td>" +
                           "<td  align='center'>" + item.partnerName + "</td>" +
                           "<td  align='center'>" + item.year + "</td>" +
                           "<td  align='center'>" + item.month + "</td>" +
                           "<td  align='center'>" + item.userCode + "</td>" +
                           "<td  align='center'>" + item.dateCreate + "</td>" +
                           "<td  align='center'>" + item.usercodeUpdate + "</td>" +
                           "<td  align='center'>" + item.dateUpdate + "</td>"

                    ;
                    rows += "</tr>";
                    $("#Tacl").append(rows);
                });

            },
            error: function (err) {

            }
        });
    }


</script>

