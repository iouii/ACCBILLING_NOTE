<div class="container-fluid">
    <div class="row">
        @if (ViewBag.calco == 0)
        {
            @Html.Partial("_ExPartial")
        }
        else if (ViewBag.calco == 1)
        {
            @Html.Partial("_Ex1Partial")
        }
    </div>
</div>

<script>

    function printDiv(id) {
        if (confirm("You sure save this data?")) {
            saveDataSumary();
        }
        var printContents = document.getElementById(id).innerHTML;
        var originalContents = document.body.innerHTML;
        document.body.innerHTML = printContents;
        window.print();
        document.body.innerHTML = originalContents;
    }
    function saveDataSumary() {

        //set value
        var arrDuedate = "", dateprint = "";
        var OnDoc = $("#TreiTwo").text();
        arrDuedate = $("#tbduedate").text();
        dateprint = $("#pirntDateNow").text();
        //get value

        var jsonStr = "@Session["saveDataExort"]";
        // console.log(arrDuedate + ' - ' + arrDateNow);
        var jsonData = JSON.parse(jsonStr.replace(/&quot;/g,'"'));

        //loop push json
        $.each(jsonData, function (i, item) {
            jsonData[i].ondoc = OnDoc;
            jsonData[i].duedate = arrDuedate;
            jsonData[i].dateprints = dateprint;
        });
        var jsonString = JSON.stringify(jsonData);
        $.ajax({
            url: "/SaveData/SaveData",
            type: "POST",
            data: { jsonData: jsonString },
            datatype: "json",
            success: function (data) {
                alert(data);

            }
        });



    }


</script>

<script>

    function editCur(r) {

        var i = r.parentNode.rowIndex;

        var table = document.getElementById("tableOc");
        var tableP = document.getElementById("tableOP");

        var e = table.rows[i].cells[2].innerText;

        var al = prompt('Would you like to change currency?', e)

        var af = table.rows[i].cells[2].innerHTML = al;
        var changeDue = tableP.rows[i].cells[2].innerHTML = al;

    }
</script>

